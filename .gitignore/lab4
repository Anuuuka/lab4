#include <stdio.h>
#include <stdlib.h>
#include <math.h>

int main()
{
   double x;
   double y;
   printf("  x  |  y \n");
   x=1;
    while(x>=-1)
   {
       printf(" %.1lf | ", x);
       x=x-0.1;
       y=pow(x,3);
       printf(" %.3lf\n", y);
   }

    return 0;
}
